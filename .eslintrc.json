{
    "extends": "google",
    "installedESLint": true,
    "rules": {
        "prefer-arrow-callback": "off",
        "comma-dangle": ["error", "never"],
        "new-cap": ["error", { "newIsCap": false }],
        "no-console": ["error", {
          "allow": ["info", "warn", "error"]
        }]
    }
}
